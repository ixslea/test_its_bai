{
  "version": 2,
  "builds": [
        {
            "src": "test_its_bai/wsgi.py",
            "use": "@vercel/python",
            "config": {
                "maxLambdaSize": "15mb",
                "runtime": "python3.11.3"
            }
        }
    ],
    "routes": [
        {
        "src": "/static/(.*)",
        "dest": "test_its_bai/static/$1"
        },
        {
            "src": "/(.*)",
            "dest": "test_its_bai/wsgi.py"
        }
    ]
}